{
  "name": "airstack-explorer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run codegen-watch & next dev",
    "build": "next build",
    "build-sh": "sh ./build.sh",
    "start": "next start",
    "lint": "next lint --max-warnings 0",
    "dev-server": "next dev",
    "prebuild": "npm run codegen && npm run lint && npm run ts-lint",
    "build-uat": "npm run codegen && tsc && vite build --mode uat",
    "build-dev": "npm run codegen && tsc && vite build --mode development",
    "ts-lint": "tsc --noEmit --incremental",
    "preview": "vite preview",
    "prepare": "husky install",
    "pre-commit": "npm run codegen && npm run lint && npm run ts-lint",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen-watch": "graphql-codegen --watch --config codegen.ts",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@airstack/airstack-react": "0.6.2",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@privy-io/react-auth": "^1.59.3-beta-20240308185032",
    "classnames": "^2.3.2",
    "comlink": "^4.4.1",
    "next": "14.1.1",
    "react": "^18",
    "react-calendar": "^4.6.0",
    "react-dom": "^18",
    "react-infinite-scroll-component": "^6.1.0",
    "react-modal": "^3.16.1",
    "react-nano-store": "^0.1.3",
    "react-router-dom": "^6.14.1",
    "substyle": "^9.4.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-modal": "^3.16.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.1",
    "husky": "^8.0.0",
    "patch-package": "^8.0.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
